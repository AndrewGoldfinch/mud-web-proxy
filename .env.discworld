# Discworld MUD E2E Configuration
# Copy this to .env.discworld.local and add your credentials

# Enable this MUD for testing
DISCWORLD_ENABLED=true

# Connection settings
DISCWORLD_HOST=discworld.starturtle.net
DISCWORLD_PORT=4242

# Authentication (add to .env.discworld.local, not this file!)
DISCWORLD_USERNAME=
DISCWORLD_PASSWORD=
DISCWORLD_CHARACTER=

# Protocol expectations
DISCWORLD_EXPECT_GMCP=false
DISCWORLD_EXPECT_MCCP=false
DISCWORLD_EXPECT_MXP=true
DISCWORLD_EXPECT_MSDP=false
DISCWORLD_EXPECT_UTF8=true
DISCWORLD_EXPECT_ANSI=true

# Test settings
DISCWORLD_TIMEOUT_MS=30000
DISCWORLD_LOGIN_PROMPT="Enter your username:"
