# Discworld MUD E2E Configuration
# Copy this to .env.discworld.local and add your credentials

# Enable this MUD for testing
ENABLED=true

# Connection settings
HOST=discworld.starturtle.net
PORT=4242

# Authentication (add to .env.discworld.local, not this file!)
USERNAME=
PASSWORD=
CHARACTER=

# Protocol expectations
EXPECT_GMCP=false
EXPECT_MCCP=false
EXPECT_MXP=true
EXPECT_MSDP=false
EXPECT_UTF8=true
EXPECT_ANSI=true

# Test settings
TIMEOUT_MS=30000
LOGIN_PROMPT="Enter your username:"
