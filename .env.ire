# IRE MUD E2E Configuration (Achaea/Imperian/Lusternia/Aetolia)
# Copy this to .env.ire.local and add your credentials

# Enable this MUD for testing
IRE_ENABLED=true

# Connection settings (change to your specific IRE MUD)
IRE_HOST=achaea.com
IRE_PORT=23

# Authentication (add to .env.ire.local, not this file!)
IRE_USERNAME=
IRE_PASSWORD=
IRE_CHARACTER=

# Protocol expectations
IRE_EXPECT_GMCP=true
IRE_EXPECT_MCCP=true
IRE_EXPECT_MXP=false
IRE_EXPECT_MSDP=false
IRE_EXPECT_UTF8=true
IRE_EXPECT_ANSI=true

# Test settings
IRE_TIMEOUT_MS=30000
IRE_LOGIN_PROMPT="Enter your username:"
