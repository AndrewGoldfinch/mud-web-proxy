# Raw Telnet Server E2E Configuration
# Copy this to .env.raw.local and add your credentials

# Enable this MUD for testing
RAW_ENABLED=true

# Connection settings (update for your raw telnet server)
RAW_HOST=your-telnet-server.com
RAW_PORT=23

# Authentication (add to .env.raw.local, not this file!)
RAW_USERNAME=
RAW_PASSWORD=

# Protocol expectations (minimal protocols for raw telnet)
RAW_EXPECT_GMCP=false
RAW_EXPECT_MCCP=false
RAW_EXPECT_MXP=false
RAW_EXPECT_MSDP=false
RAW_EXPECT_UTF8=false
RAW_EXPECT_ANSI=true

# Test settings
RAW_TIMEOUT_MS=30000
RAW_LOGIN_PROMPT="login:"
