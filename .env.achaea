# Achaea MUD E2E Configuration
# Copy this to .env.achaea.local and add your credentials

# Enable this MUD for testing
ENABLED=true

# Connection settings
HOST=achaea.com
PORT=23

# Authentication (add to .env.achaea.local, not this file!)
USERNAME=
PASSWORD=
CHARACTER=

# Protocol expectations
EXPECT_GMCP=true
EXPECT_MCCP=true
EXPECT_MXP=false
EXPECT_MSDP=false
EXPECT_UTF8=true
EXPECT_ANSI=true

# Test settings
TIMEOUT_MS=30000
LOGIN_PROMPT="Enter your username:"
