# Achaea MUD E2E Configuration
# Copy this to .env.achaea.local and add your credentials

# Enable this MUD for testing
ACHAEA_ENABLED=true

# Connection settings
ACHAEA_HOST=achaea.com
ACHAEA_PORT=23

# Authentication (add to .env.achaea.local, not this file!)
ACHAEA_USERNAME=
ACHAEA_PASSWORD=
ACHAEA_CHARACTER=

# Protocol expectations
ACHAEA_EXPECT_GMCP=true
ACHAEA_EXPECT_MCCP=true
ACHAEA_EXPECT_MXP=false
ACHAEA_EXPECT_MSDP=false
ACHAEA_EXPECT_UTF8=true
ACHAEA_EXPECT_ANSI=true

# Test settings
ACHAEA_TIMEOUT_MS=30000
ACHAEA_LOGIN_PROMPT="Enter your username:"
