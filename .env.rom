# ROM MUD E2E Configuration
# Copy this to .env.rom.local and add your credentials

# Enable this MUD for testing
ROM_ENABLED=true

# Connection settings (update for your ROM MUD)
ROM_HOST=your-rom-mud.com
ROM_PORT=4000

# Authentication (add to .env.rom.local, not this file!)
ROM_USERNAME=
ROM_PASSWORD=
ROM_CHARACTER=

# Protocol expectations
ROM_EXPECT_GMCP=false
ROM_EXPECT_MCCP=false
ROM_EXPECT_MXP=false
ROM_EXPECT_MSDP=false
ROM_EXPECT_UTF8=true
ROM_EXPECT_ANSI=true

# Test settings
ROM_TIMEOUT_MS=30000
ROM_LOGIN_PROMPT="Login:"
