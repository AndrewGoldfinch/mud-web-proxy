# Aardwolf MUD E2E Configuration
# Copy this to .env.aardwolf.local and add your credentials

# Enable this MUD for testing
ENABLED=true

# Connection settings
HOST=aardmud.org
PORT=4000

# Authentication (add to .env.aardwolf.local, not this file!)
USERNAME=
PASSWORD=
CHARACTER=

# Protocol expectations
EXPECT_GMCP=true
EXPECT_MCCP=true
EXPECT_MXP=false
EXPECT_MSDP=false
EXPECT_UTF8=true
EXPECT_ANSI=true

# Test settings
TIMEOUT_MS=30000
LOGIN_PROMPT="Enter your username:"
